{
  "name": "syphonx",
  "version": "1.2.64",
  "description": "SyphonX is a tool that extracts data from HTML data, transforming it into JSON of any shape or size. It combines the power of CSS Selectors and jQuery, Regular Expressions, and Javascript into a declarative template format to elegantly solve the simplest to the most complex data extraction problems.",
  "author": "Dave Templin <dtempx@gmail.com> (https://github.com/dtempx/)",
  "homepage": "https://github.com/dtempx/syphonx",
  "repository": "github:dtempx/syphonx",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "syphonx": "./dist/command.js"
  },
  "files": [
    "./dist/"
  ],
  "scripts": {
    "build": "npx tsc",
    "clean": "bash ./clean.sh",
    "test": "npx mocha"
  },
  "keywords": [
    "automation",
    "cheerio",
    "crawl",
    "crawler",
    "crawling",
    "chrome",
    "dom",
    "headless",
    "html",
    "html2json",
    "jquery",
    "parse",
    "parser",
    "puppeteer",
    "regex",
    "scrape",
    "scraper",
    "scraping",
    "spider",
    "spidering",
    "transform",
    "web",
    "web crawler",
    "web crawling",
    "web scraper",
    "web scraping"
  ],
  "dependencies": {
    "@google-cloud/bigquery": "^6.0.3",
    "@types/jquery": "^3.5.14",
    "@types/node": "^18.7.18",
    "@types/parse-full-name": "^1.2.2",
    "@types/uuid": "^8.3.4",
    "async-file": "^2.0.2",
    "async-parallel": "^1.2.3",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.0.0",
    "jquery": "^3.6.0",
    "json5": "^2.2.1",
    "nanoid": "^4.0.0",
    "parse-full-name": "^1.2.6",
    "playwright": "^1.42.0",
    "syphonx-lib": "^1.2.61",
    "typescript": "^4.8.3",
    "word-wrap": "^1.2.3"
  }
}
